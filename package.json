{
  "apiHostname": "http://please.define",
  "name": "gusdewa",
  "version": "0.0.1",
  "author": "gusdewa",
  "description": "React Web Interface to display WordPress data through WP Rest API",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "npm run lint:prod && npm run test && node tools/scripts/build.js",
    "start": "node tools/scripts/start.js",
    "start:prod": "node tools/scripts/startProd.js",
    "lint": "eslint src/",
    "lint:prod": "eslint -c tools/eslint.prod.js src/",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/help/**/*",
      "!src/**/*.test.js"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 28,
        "branches": 20,
        "functions": 24,
        "lines": 29
      }
    },
    "globals": {
      "CUSTOMAPIHOST": "mockApiHost",
      "VERSION": "1.1.1",
      "__DEV__": true
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      ".*\\.css$": "<rootDir>/tools/testing/mocks/css.js",
      ".*\\.scss$": "<rootDir>/tools/testing/mocks/scss.js",
      ".*\\.(jpg|png)$": "<rootDir>/tools/testing/mocks/image.js"
    },
    "setupFiles": [
      "./tools/setupTest.js"
    ],
    "setupTestFrameworkScriptFile": "./tools/setupTestFramework.js",
    "testRegex": "tests/.*\\.test\\.(js|jsx)$",
    "verbose": true,
    "timers": "fake"
  },
  "dependencies": {
    "babel-polyfill": "6.20.0",
    "classnames": "2.2.5",
    "jquery": "3.1.1",
    "lodash": "4.17.2",
    "material-ui": "0.16.5",
    "moment": "2.17.1",
    "react-bootstrap": "0.30.7",
    "react-datepicker": "0.39.0",
    "react-dom": "15.4.1",
    "react-intl": "2.1.5",
    "react-redux": "5.0.1",
    "react-router-redux": "4.0.7",
    "react-router": "3.0.0",
    "react-select": "1.0.0-rc.2",
    "react-slick": "0.14.5",
    "react-tap-event-plugin": "2.0.1",
    "redux-immutable-state-invariant": "1.2.4",
    "react": "15.4.1",
    "redux-thunk": "2.1.0",
    "redux": "3.6.0",
    "slick-carousel": "1.6.0",
    "toastr": "2.1.2"
  },
  "devDependencies": {
    "autoprefixer": "6.5.4",
    "babel-cli": "6.18.0",
    "babel-core": "6.21.0",
    "babel-eslint": "7.1.1",
    "babel-jest": "18.0.0",
    "babel-loader": "6.2.10",
    "babel-plugin-react-intl": "2.2.0",
    "babel-preset-latest": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-register": "6.18.0",
    "chalk": "1.1.3",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "0.26.1",
    "enzyme": "2.6.0",
    "eslint": "3.12.2",
    "eslint-config-airbnb": "13.0.0",
    "eslint-import-resolver-webpack": "0.8.0",
    "eslint-loader": "1.6.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.8.0",
    "express": "4.14.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "filesize": "3.3.0",
    "fs-extra": "1.0.0",
    "gzip-size": "3.0.0",
    "html-loader": "0.4.4",
    "html-webpack-plugin": "2.24.1",
    "jest": "18.0.0",
    "mockdate": "2.0.1",
    "node-sass": "4.0.0",
    "open-browser-webpack-plugin": "0.0.3",
    "postcss-loader": "1.2.1",
    "react-addons-test-utils": "15.4.1",
    "react-dev-utils": "0.4.2",
    "react-test-renderer": "15.4.1",
    "resolve-url-loader": "1.6.1",
    "rimraf": "2.5.4",
    "sass-loader": "4.1.0",
    "strip-ansi": "3.0.1",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.14.0",
    "webpack-bundle-analyzer": "2.1.1",
    "webpack-dev-middleware": "1.9.0",
    "webpack-dev-server": "1.16.2",
    "webpack-hot-middleware": "2.13.2",
    "webpack-md5-hash": "0.0.5",
    "webpack-validator": "2.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gusdewa/react-wp"
  }
}
